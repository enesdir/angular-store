    "prettier:verify": "prettier --config ./.prettierrc.cjs --check \"src/{app,environments}/**/*{.ts,.html,.scss,.json}\"",
    "serve:ssr:angular-store": "node dist/angular-store/server/server.mjs",
    "start": "ng serve ",
    "test": "ng test",
    "watch": "ng build --watch --configuration development"
  },
