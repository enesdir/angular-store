	"scripts": {
		"build": "ng build",
		"lint": "ng lint",
		"lint:fix": "ng lint --fix",
		"ng": "ng",
		"prettier": "prettier --config ./.prettierrc.cjs --write \"src/{app,environments}/**/*{.ts,.html,.scss,.json}\"",
