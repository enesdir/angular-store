{
  "name": "angular-store",
  "version": "0.3.0",
  "displayName": "Angular Store",
  "description": "Angular & Tailwind CSS Starter Kit, Free and Open Source",
  "private": true,
  "engines": {
    "node": ">=18.x.x",
    "npm": ">=9"
  },
  "scripts": {
    "build": "ng build",
    "lint": "ng lint",
